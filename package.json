{
  "name": "reqres-application",
  "version": "0.0.1",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --open --config webpack.dev.js",
    "start:no-open": "webpack-dev-server --config webpack.dev.js",
    "build": "webpack --config webpack.prod.js",
    "test": "jest --passWithNoTests --env=jsdom",
    "test:watch": "jest --watch --env=jsdom",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "prettify": "prettier --write .",
    "typecheck": "tsc",
    "prepare": "husky install",
    "cypress-open": "./node_modules/.bin/cypress open",
    "cypress-run": "./node_modules/.bin/cypress run",
    "cypress-local": "concurrently \"npm run start:no-open\" \"npm run cypress-open\" \"npm run cypress-run\"",
    "e2e": "concurrently \"npm run start:no-open\" \"npm run cypress-run\""
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.6.1",
    "axios": "^0.21.4",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.5",
    "react-router-dom": "^5.3.0",
    "redux": "^4.1.1",
    "styled-components": "^5.3.1"
  },
  "devDependencies": {
    "@svgr/webpack": "^5.5.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.0",
    "@testing-library/react-hooks": "^7.0.2",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.9.2",
    "@types/react": "^17.0.21",
    "@types/react-dom": "^17.0.9",
    "@types/react-router-dom": "^5.1.9",
    "@types/styled-components": "^5.1.14",
    "@types/webpack": "^5.28.0",
    "@types/webpack-dev-server": "^4.1.0",
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "@typescript-eslint/parser": "^4.31.1",
    "concurrently": "^6.2.1",
    "css-loader": "^6.2.0",
    "cypress": "^8.4.1",
    "eslint": "^7.32.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.25.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-tsdoc": "^0.2.14",
    "eslint-plugin-unused-imports": "^1.1.4",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.3.2",
    "husky": "^7.0.0",
    "jest": "^27.2.0",
    "prettier": "2.4.1",
    "start-server-and-test": "^1.14.0",
    "style-loader": "^3.2.1",
    "ts-jest": "^27.0.5",
    "ts-loader": "^9.2.5",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.3",
    "typescript-plugin-styled-components": "^2.0.0",
    "webpack": "^5.53.0",
    "webpack-bundle-analyzer": "^4.4.2",
    "webpack-cli": "^4.8.0",
    "webpack-dev-server": "^4.2.1",
    "webpack-merge": "^5.8.0"
  }
}
